{
  "name": "lodash-walk",
  "version": "1.0.0",
  "description": "Lodash walk",
  "main": "dist/index.min.js",
  "files": [
    "dist/*",
    "index.js"
  ],
  "scripts": {
    "test": "mocha tests",
    "index:build:dev": "browserify --debug index.js | exorcist dist/index.js.map > dist/index.js",
    "index:build:min": "browserify -g uglifyify --debug index.js | exorcist dist/index.min.js.map > dist/index.min.js",
    "index:build": "npm run index:build:dev && npm run index:build:min",
    "walk:build:dev": "browserify --debug walk.js | exorcist dist/walk.js.map > dist/walk.js",
    "walk:build:min": "browserify -g uglifyify --debug walk.js | exorcist dist/walk.min.js.map > dist/walk.min.js",
    "walk:build": "npm run walk:build:dev && npm run walk:build:min",
    "build": "npm run index:build && npm run walk:build",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielmoreira/lodash-walk.git"
  },
  "keywords": [
    "lodash",
    "walk",
    "deep"
  ],
  "author": "Gabriel Moreira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabrielmoreira/lodash-walk/issues"
  },
  "homepage": "https://github.com/gabrielmoreira/lodash-walk#readme",
  "dependencies": {
    "lodash": "^4.16.4"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "chai": "^3.5.0",
    "exorcist": "^0.4.0",
    "mocha": "^3.1.2",
    "uglifyify": "^3.0.4"
  }
}
